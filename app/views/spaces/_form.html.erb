<%= form_for @space do |f| %>
	<p><%= f.label :name %>
		<%= f.text_field :name%></p>

	<p><%= f.label :category %>
		<%= f.text_field :category%></p>

	<p><%= f.label :description %>
		<%= f.text_area :description%></p>

	<p><%= f.label :location %>
		<%= f.text_field :location%></p>

	<p><%= f.label :price %>
		<%= f.text_field :price%></p>

	<p><%= f.label :capacity %>
		<%= f.text_field :capacity%></p>
		
	<p><%= f.submit %></p>
<% end %>